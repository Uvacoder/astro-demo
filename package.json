{
  "name": "astro-demo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build-images && astro dev",
    "build": "npm run build-images && astro build",
    "build-images": "node build-images.js",
    "format": "prettier --write \"src/**/*.{js,md}\" \"public/global.css\" \"astro.config.mjs\" \"tailwind.config.js\""
  },
  "devDependencies": {
    "astro": "^0.18.9",
    "prettier": "^2.4.0",
    "tailwindcss": "^2.1.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@sindresorhus/slugify": "^2.1.0",
    "responsiv-image-handler": "^1.1.0"
  }
}
